{
  "name": "js-beans",
  "version": "1.1.3",
  "description": "IoC in Spring style",
  "author": "Arsenii_Boretskii <arseniiboretskii@gmail.com>",
  "homepage": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "src/__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ArseniyBorezkiy/js-beans"
  },
  "scripts": {
    "build": "rimraf dist && tsc && babel dist -d dist",
    "test": "jest --no-cache",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "@types/fetch-mock": "^7.2.3",
    "@types/jest": "24.0.11",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-preset-es2015": "6.24.1",
    "fetch-mock": "^7.3.1",
    "jest": "23.6.0",
    "jest-fetch-mock": "^2.1.2",
    "ts-jest": "23.10.5",
    "tslib": "^1.9.3",
    "typescript": "3.2.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts)$",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json",
        "diagnostics": false,
        "compilerOptions": {
          "importHelpers": true
        }
      }
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ]
  },
  "keywords": [
    "spring-core",
    "beans"
  ],
  "license": "ISC"
}
