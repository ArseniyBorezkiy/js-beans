{
  "name": "js-beans",
  "version": "1.0.0",
  "description": "IoC in Spring style",
  "author": "Arsenii_Boretskii <arseniiboretskii@gmail.com>",
  "homepage": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "test": "src/__tests__"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://OUT-Boretskiy-AP3@sbtatlas.sigma.sbrf.ru/stash/scm/sdsdh/sds-front.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc && babel dist -d dist",
    "test": "jest --no-cache",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "reflect-metadata": "0.1.12"
  },
  "devDependencies": {
    "typescript": "3.2.2",
    "babel-core": "^6.26.3",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.(ts)$",
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json",
        "diagnostics": false
      }
    },
    "testPathIgnorePatterns": [
      "node_modules",
      "dist"
    ]
  },
  "keywords": [
    "spring-core",
    "beans"
  ],
  "license": "ISC"
}
